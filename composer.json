{
    "name": "ttek/tkwiki",
    "type": "project",
    "description": "A basic wiki for documentations",
    "keywords": [
        "tropotek"
    ],
    "time": "2021-07-12",
    "license": "GPL",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Michael Mifsud",
            "email": "info@tropotek.com.au",
            "homepage": "http://www.tropotek.com/"
        }
    ],
    "autoload": {
        "psr-0": {
            "": "src/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "Tk\\Composer\\SetupEvent::postInstall"
        ],
        "post-update-cmd": [
            "Tk\\Composer\\SetupEvent::postUpdate"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-trunk": "3.0.x-dev"
        }
    },
    "require": {
        "php": "^7.3.0",
        "ttek/tk-base": "~3.2",
        "ext-pdo": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-tidy": "*",
        "ext-dom": "*"
    },
    "require-dev": {
    }
}
