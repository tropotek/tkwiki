{
    "name": "ttek/tkwiki",
    "type": "project",
    "description": "A basic wiki for documentations",
    "keywords":  [
        "tropotek"
    ],
    "time": "2020-02-12",
    "license": "GPL",
    "minimum-stability": "stable",
    "authors":  [
         {
            "name": "Michael Mifsud",
            "email": "info@tropotek.com.au",
            "homepage": "http://www.tropotek.com/"
        }
    ],
    "autoload":  {
        "psr-0":  {
            "": "src/"
        }
    },
    "scripts":  {
        "post-install-cmd":  [
            "Tk\\Composer\\SetupEvent::postInstall"
        ],
        "post-update-cmd":  [
            "Tk\\Composer\\SetupEvent::postUpdate"
        ]
    },
    "extra":  {
        "branch-alias":  {
            "dev-trunk": "3.0.x-dev"
        }
    },
    "require":  {
        "php": "^7.1.3",
        "ext-json": "*",
        "ttek/tk-base": "~3.0",
        "ext-tidy": "*",
        "ext-dom": "*",
        "ext-libxml": "*"
